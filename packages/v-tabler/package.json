{
    "name": "@s3_dse/v-tabler",
    "version": "1.13.2",
    "author": {
        "name": "Sebastian Doerl",
        "email": "sebastian.doerl@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/s3dse/v-tabler.git"
    },
    "type": "module",
    "license": "MIT",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/v-tabler.cjs",
    "module": "dist/v-tabler.js",
    "exports": {
        ".": {
            "import": "./dist/v-tabler.js",
            "require": "./dist/v-tabler.cjs"
        },
        "./preset": {
            "import": "./dist/preset.js",
            "require": "./dist/preset.cjs"
        },
        "./*.css": "./dist/*.css"
    },
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build && unocss \"src/**/*.{vue,js,ts}\" -o dist/styles.css",
        "demo:dev": "vite --mode demo",
        "demo:build": "vite build --mode demo",
        "demo:preview": "vite preview --mode demo",
        "test:unit:watch": "vitest watch --environment jsdom",
        "test:unit": "vitest run --environment jsdom",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "story:dev": "histoire dev",
        "story:build": "histoire build",
        "story:preview": "histoire preview"
    },
    "devDependencies": {
        "@histoire/plugin-vue": "^1.0.0-alpha.3",
        "@iconify-json/tabler": "^1.2.22",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vitest/coverage-v8": "^3.2.4",
        "@vue/test-utils": "^2.4.6",
        "histoire": "^1.0.0-alpha.3",
        "jsdom": "^26.1.0",
        "unocss-preset-scrollbar": "^3.2.0",
        "unocss-preset-theme": "^0.14.1",
        "vite": "^7.1.2",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^3.2.4",
        "vue": "^3.5.18"
    },
    "dependencies": {
        "@vueuse/components": "^13.7.0",
        "@vueuse/core": "^13.7.0",
        "lodash": "^4.17.21",
        "reka-ui": "^2.4.1",
        "vue3-popper": "^1.5.0"
    }
}
